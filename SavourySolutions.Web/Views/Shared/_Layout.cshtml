@using SavourySolutions.Common
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Site Metas -->
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@this.ViewData["Title"] - @GlobalConstants.SystemName</title>
    <!-- Site Icons -->
    <link href="~/images/favicon.ico" rel="shortcut icon" type="image/x-icon" asp-append-version="true">
    <!-- Site CSS -->
    <link href="~/lib/fontawesome/css/all.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/admin-panel.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/site.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/style.css" rel="stylesheet" asp-append-version="true" />

    <!-- Responsive CSS -->
    <link href="~/css/responsive.css" rel="stylesheet" asp-append-version="true">
    <!-- Custom CSS -->
    <link href="~/css/custom.css" rel="stylesheet" asp-append-version="true">
    <!-- Bootstrap-select CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/subscription.css" asp-append-version="true" />
    <link href="~/css/lightbox.css" rel="stylesheet" asp-append-version="true">

</head>
<body class="no-ovfl">
    <div class="preloader"></div>
    <header class="top-navbar">
        <partial name="_NavbarPartial" />
    </header>

    <div class="container-fluid nav-bug no-padding">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="no-ovfl">
            @this.RenderBody()
        </main>
    </div>

    <footer class="footer-area bg-f">
        <partial name="_FooterPartial" />
    </footer>

    <script src="~/js/popper.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js" integrity="sha512-d9xgZrVZpmmQlfonhQUvTR7lMPtO7NkZMkA0ABN3PHCbKA5nqylQ/yWlFAyY6hYgdF1Qh6nYiuADWwKB4C2WSw==" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/bootstrap-select.min.js"></script>

    <!-- ALL PLUGINS -->
    <script src="~/js/jquery.superslides.min.js" asp-append-version="true"></script>
    <script src="~/js/baguetteBox.min.js" asp-append-version="true"></script>
    <script src="~/js/custom.js" asp-append-version="true"></script>
    <script src="~/js/lightbox.js" asp-append-version="true"></script>
    @await this.RenderSectionAsync("Scripts", required: false)
</body>
</html>
